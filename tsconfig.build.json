{
  "display": "Lib Base",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "importHelpers": false,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": false,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "declarationDir": "types"
  }
}